    "nodemon": "^2.0.15",
    "redux-devtools": "^3.7.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.0",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  }
