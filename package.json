  "description": "",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon --watch src/index.js",
    "tsc": "tsc --rootDir sources/nest/ts --outDir sources/nest/js",
    "tsc-watch": "tsc --watch --rootDir sources/nest/ts --outDir sources/nest/js",
    "build": "webpack",
    "build-watch": "webpack --watch",
    "css": "node-sass --output-style compressed sources/nest/scss/main --output src/public/css",
    "css-watch": "node-sass --watch --output-style compressed sources/nest/scss/main --output src/public/css",
    "css:react": "node-sass --output-style compressed sources/nest/scss/react --output sources/nest/react",
    "css:react-watch": "node-sass --watch --output-style compressed sources/nest/scss/react --output sources/nest/react",
    "css:react-redux": "node-sass --output-style compressed sources/nest/scss/react-redux --output sources/nest/react-redux",
    "css:react-redux-watch": "node-sass --watch --output-style compressed sources/nest/scss/react-redux --output sources/nest/react-redux"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
